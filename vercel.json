{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/webhook",
            "dest": "/api/webhook"
        }
    ],
    "env": {
        "BOT_TOKEN": "@bot_token",
        "ADMIN_IDS": "@admin_ids",
        "DATABASE_PATH": "/tmp/db.sqlite"
    }
}